# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                                       Application.index
GET     /channel                                Application.channel
GET     /test/?                                 Application.saveUser
GET     /indexDeez/{user}                       Application.indexDeez
GET     /test2/                                 Application.connectUser
GET     /getSpotAccessToken                     Application.spotConnection
GET     /putSpotAlbums                          Application.putSpotAlbums
GET     /getDeezAlbums                          Application.getGson
# Ignore favicon requests
GET     /favicon.ico                            404

# Map static resources from the /app/public folder to the /public path
GET     /public/                                staticDir:public

# Catch all
*       /{controller}/{action}                  {controller}.{action}
